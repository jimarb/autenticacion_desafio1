<!DOCTYPE html>
<html>
  <head>
    <title>Pictory</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <% if logged? %>
      <p><%= current_user.name %></p>
        <%= link_to "Cerrar Sesion", session_path(current_user.id), method: :delete %>
      <% else %>

        <%= link_to "Iniciar Sesion" %>
      <% end %>
    <%= yield %>
  </body>
</html>
